{
  "name": "quickmarket/app",
  "description": "Simple routing system with Composer autoloading",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Config\\": "config/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "fakerphp/faker": "^1.24"
  },
  "scripts": {
    "seed": "php scripts/seed.php",
    "seed:fresh": "php scripts/seed.php --fresh"
  }
}
